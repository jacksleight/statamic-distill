{
    "name": "jacksleight/statamic-distill",
    "autoload": {
        "psr-4": {
            "JackSleight\\StatamicDistill\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Distill",
            "description": "This Statamic addon allows you to filter, fetch or index the individual values, sets and relations within your entries, from both root and deeply nested fields."
        },
        "laravel": {
            "providers": [
                "JackSleight\\StatamicDistill\\ServiceProvider",
                "JackSleight\\StatamicDistill\\StillServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^3.4 || ^4.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
